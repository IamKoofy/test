import json
from openpyxl import Workbook

# Replace with your JSON file path
json_file = "data.json"

# Open the JSON file
with open(json_file, "r") as infile:
    user_data = json.load(infile)

# Create an Excel workbook
wb = Workbook()
sheet = wb.active

# Set headers based on user data structure (modify if needed)
header_row = ["Username", "Email", "First Name", "Last Name"]  # Adjust based on your data keys
sheet.append(header_row)

# Write user data to Excel sheet
for user in user_data:
    user_row = [user.get(key, "") for key in header_row]  # Handle potential missing keys
    sheet.append(user_row)

# Save the Excel file
wb.save("users.xlsx")

print("User data exported to users.xlsx")
