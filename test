import json
from openpyxl import Workbook

# Load JSON data from file
with open('users.json', 'r') as json_file:
    users_data = json.load(json_file)

# Create Excel workbook
wb = Workbook()
sheet = wb.active

# Set headers
sheet.cell(row=1, column=1).value = "Username"
sheet.cell(row=1, column=2).value = "Email"

# Write user data
for index, user in enumerate(users_data, start=2):
    sheet.cell(row=index, column=1).value = user.get("username", "")
    sheet.cell(row=index, column=2).value = user.get("email", "")

# Save the Excel file
wb.save("awx_users.xlsx")

print("User list exported to awx_users.xlsx")
